getgenv().autoRebirth = false;
getgenv().autoTHROW = false;
getgenv().autoBuy = false;
getgenv().autoBuy2 = false;
getgenv().autoBuy3 = false;
getgenv().autoBuy4 = false;
getgenv().buyEgg = false;
getgenv().thisvalue = 'my name';


local remotePath = game:GetService("ReplicatedStorage").Remotes;
local player = game:GetService("CoreGui")


local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/Rain-Design/Unnamed/main/Library.lua'))()


