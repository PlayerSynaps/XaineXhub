--Functoion
--  --
--  -- 
--  --
 --------


--AutoRebirth
function autoRebirth()
spawn(function()
while getgenv().autoRebirth == true do
    remotePath.RebirthRequest:InvokeServer()
    wait()
end
end)
end

--AutoStreng
--1 power
function buyStrength()
spawn(function()
    while getgenv().autoBuy == true do
        local args = {[1] = 1}
        remotePath.BuyStrength:FireServer(unpack(args))
        wait(0.5)
    end
    end)
    end
--100 power
function buyPower()
spawn(function()
    while getgenv().autoBuy2 == true do
        local args = {[1] = 2}
        remotePath.BuyStrength:FireServer(unpack(args))
        wait(0.5)
    end
    end)
end
--1000 power
function buySPower()
spawn(function ()
    while getgenv().autoBuy3 == true do
        local args = {[1] = 3}
        remotePath.BuyStrength:FireServer(unpack(args))
        wait(0.5)
    end 
    end)
end
--100.000 power
function buyPSower()
    spawn(function ()
        while getgenv().autoBuy4 == true do
            local args = {[1] = 4}
            remotePath.BuyStrength:FireServer(unpack(args))
            wait(0.25)
        end 
        end)
    end
--AutoEgg
function buyEgg(eggType, eggLimit )
spawn(function ()
    local iteration = 0;
    while wait() do
        if not buyEgg or iteration == eggLimit then break end;
        local args = {[1] = "Emerald Crate"}   
        remotePath.OpenCrate:InvokeServer(eggType)
        iteration = iteration +1
    end
end)
end
--PlayerTP
function getCurrentPlayerPOS()
    local plyr = game.Players.LocalPlayer;
    if plyr.Character then
        return plyr.Character.HumanoidRootPart,Position;
    end
    return false;
end
--THROW
function autoThrow()
    spawn(function ()
        while getgenv().autoTHROW == true do
            remotePath.AttemptThrow:InvokeServer()
            wait()
        end
    end)
end



